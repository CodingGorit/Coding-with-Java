<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.gorit.entity.ClasszEntity" table="classz" schema="studentmanagementsystem">
        <id name="stuId">
            <column name="stuId" sql-type="varchar(20)" length="20"/>
        </id>
        <property name="grade">
            <column name="grade" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="clasz">
            <column name="clasz" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>